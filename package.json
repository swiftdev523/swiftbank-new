{
  "name": "swift-bank",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:netlify": "CI=true NETLIFY=true vite build --mode production",
    "build:safe": "vite build --mode production",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "powershell -Command \"if (Test-Path dist) { Remove-Item -Recurse -Force dist }; if (Test-Path node_modules/.vite) { Remove-Item -Recurse -Force node_modules/.vite }\"",
    "clean:cache": "powershell -Command \"if (Test-Path node_modules/.vite) { Remove-Item -Recurse -Force node_modules/.vite }; if (Test-Path dist) { Remove-Item -Recurse -Force dist }\"",
    "build:prod": "npm run clean:cache && npm run type-check && vite build --mode production",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "firebase:emulators": "firebase emulators:start",
    "firebase:deploy": "npm run build:prod && firebase deploy",
    "firebase:deploy:hosting": "npm run build:prod && firebase deploy --only hosting",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules",
    "firebase:deploy:functions": "firebase deploy --only functions",
    "firebase:deploy:prod": "powershell -ExecutionPolicy Bypass -File ./deploy.ps1",
    "firebase:login": "firebase login",
    "firebase:init": "firebase init",
    "start:prod": "npm run build:prod && npm run preview",
    "test:firebase": "firebase emulators:exec --only firestore,auth 'npm run test'",
    "cleanup:dry": "node scripts/cleanup-single-user.cjs --dry --customerEmail=customer@swiftbank.com --adminEmail=admin@swiftbank.com",
    "cleanup:force": "node scripts/cleanup-single-user.cjs --force --customerEmail=customer@swiftbank.com --adminEmail=admin@swiftbank.com",
    "seed:accountTypes": "node scripts/seed-account-types.cjs",
    "verify:accountTypes": "node scripts/verify-account-types.cjs",
    "seed:johnson": "node scripts/seed-johnson-web.mjs",
    "verify:johnson": "node scripts/verify-johnson-web.mjs",
    "manage:johnson": "node scripts/manage-johnson-accounts.cjs",
    "seed:users": "node scripts/seed-users.mjs",
    "verify:users": "node scripts/verify-users.mjs",
    "manage:users": "node scripts/manage-users.mjs",
    "create:developer": "node scripts/create-developer.cjs",
    "list:developers": "node scripts/create-developer.cjs --list",
    "setup:developer": "node scripts/setup-developer.mjs",
    "setup:developer-fallback": "node scripts/setup-developer.mjs --use-known-uid",
    "verify:developer": "node scripts/verify-developer-data.mjs",
    "developer:fix": "node scripts/fix-developer-data.mjs",
    "developer:stats": "node scripts/manage-developer-system.cjs --developer-stats",
    "developer:list-all": "node scripts/manage-developer-system.cjs --list-all",
    "developer:cleanup": "node scripts/manage-developer-system.cjs --cleanup-inactive",
    "developer:verify": "node scripts/manage-developer-system.cjs --verify-integrity",
    "developer:analyze": "node scripts/analyze-database.mjs",
    "developer:validate": "node scripts/validate-data.mjs",
    "developer:validate-fix": "node scripts/validate-data.mjs --fix",
    "generate:transactions": "node scripts/generate-transaction-history.cjs"
  },
  "dependencies": {
    "@firebase/app": "^0.14.3",
    "@firebase/auth": "^1.11.0",
    "@firebase/firestore": "^4.9.2",
    "@vitejs/plugin-react": "^5.0.0",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^7.0.1",
    "motion": "^12.23.16",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.2",
    "terser": "^5.44.0",
    "vite": "^7.1.2",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "minimist": "^1.2.8",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13"
  }
}
